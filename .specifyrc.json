{
  "version": "2",
  "repository": "@acme-inc/all-design-data-v2",
  "rules": [
    {
      "name": "To React Native",
      "parsers": [
        {
          "name": "to-react-native",
          "output": {
            "type": "file",
            "filePath": "tokens.ts"
          }
        }
      ]
    },
    {
      "name": "Generate SDTF Token Graph",
      "parsers": [
        {
          "name": "to-sdtf",
          "output": {
            "type": "file",
            "filePath": "graph.json"
          }
        }
      ]
    }
  ]
}
